# GitHub API (for data collection)
# Required scope: public_repo (to read public repositories for code examples)
# Create token at: https://github.com/settings/tokens
GITHUB_TOKEN=your_github_personal_access_token

# Model paths
MODEL_PATH=models/best_model.pth
TOKENIZER_PATH=models/tokenizer.pkl

# CORS settings
ALLOWED_ORIGINS=http://localhost:5173,https://claso.dev

# Supabase (for feedback storage)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key
SUPABASE_TABLE=feedback

# Optional: Weights & Biases (for training monitoring)
WANDB_API_KEY=your_wandb_api_key